{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "anna-laura",
  "main": "src/index.ts",
  "compatibility_date": "2025-10-08",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./public"
  },
  "observability": {
    "enabled": true
  },
  "ai": {
    "binding": "AI"
  },
  "bindings": [
    {
      "type": "r2_bucket",
      "name": "CHAT_BUCKET",
      "bucket_name": "chatbucket"
    },
    {
      "type": "kv_namespace",
      "name": "CHAT_BUCKET_KV",
      "namespace_id": "CHAT_BUCKET_KV"
    }
  ],
  "upload_source_maps": true
}
